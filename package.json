{
  "name": "post-style",
  "version": "1.0.2",
  "description": "CSS style for markdown",
  "source": "examples/index.html",
  "scripts": {
    "build": "gulp",
    "dev": "parcel & gulp dev",
    "build-site": "gulp cleanDist && parcel build",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "keywords": [
    "css",
    "markdown",
    "rich text",
    "style",
    "sass"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jeffwcx/post.css.git"
  },
  "author": "jeffwcx",
  "license": "MIT",
  "devDependencies": {
    "@parcel/config-default": "^2.8.3",
    "@parcel/core": "^2.8.3",
    "@parcel/transformer-raw": "^2.8.3",
    "cz-conventional-changelog": "^3.3.0",
    "del": "^7.0.0",
    "esm": "^3.2.25",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-csso": "^4.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "parcel": "^2.8.3",
    "sass": "^1.58.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
